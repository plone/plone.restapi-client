{
  "name": "@plone/client",
  "description": "JavaScript Plone RESTAPI client - JS framework agnostic library based on TanStack Query",
  "maintainers": [
    {
      "name": "Plone Foundation",
      "email": "plone-developers@lists.sourceforge.net",
      "url": "http://plone.org"
    }
  ],
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:plone/restapi-js-client.git"
  },
  "bugs": {
    "url": "https://github.com/plone/restapi-js-client/issues",
    "email": "plone-developers@lists.sourceforge.net"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/plone-client.umd.js",
  "module": "./dist/plone-client.es.js",
  "types": "./dist/plone-client.es.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plone-client.es.d.ts",
      "import": "./dist/plone-client.es.js",
      "require": "./dist/plone-client.umd.js"
    }
  },
  "homepage": "https://plone.org",
  "keywords": [
    "volto",
    "plone",
    "plone6",
    "react",
    "cms",
    "client",
    "query",
    "react-query"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest --no-threads",
    "coverage": "vitest run --coverage --no-threads",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "dry-release": "release-it --dry-run",
    "release": "release-it",
    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/superagent": "4.1.16",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.28.4",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "jsdom": "^21.1.0",
    "prettier": "2.8.3",
    "release-it": "15.6.0",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "0.28.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@tanstack/react-query": "4.24.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superagent": "8.0.9",
    "universal-cookie": "^4.0.4"
  },
  "packageManager": "yarn@3.2.3"
}
